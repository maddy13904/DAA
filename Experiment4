#include<stdio.h>
int main()
{
	int n,a[n],i;
	printf("Madhavan-192210580\n");
	printf("Enter number of elements:");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		printf("Enter element%d:",i+1);
		scanf("%d",&a[i]);
	}
	int max=a[0];
	for(i=0;i<n;i++)
	{
		if(a[i]>max)
		{
			max=a[i];
		}
	}
	printf("Maximum element in the given array=%d",max);
}

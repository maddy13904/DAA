#include<stdio.h>
#include<math.h>
int main()
{
	int n,a,b,i,sum=0,c=0;
	printf("Madhavan-192210580\n");
	printf("Enter a number:");
	scanf("%d",&n);
	a=n,b=n;
	while(a!=0)
	{
		a=a/10;
		c++;
	}
	while(b!=0)
	{
		int r=b%10;
		sum+=pow(r,c);
		b=b/10;
	}
	if(sum==n)
	{
		printf("ARMSTRONG");
	}
	else
	{
		printf("NOT ARMSTRONG");
	}
}

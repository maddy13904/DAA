#include <stdio.h>

int main() {
	printf("Madhavan-192210580\n");
    int number, originalNumber, remainder, sum = 0;
    printf("Enter an integer: ");
    scanf("%d", &number);
    originalNumber = number;
    while (number != 0) {
        remainder = number % 10;
        sum += remainder;
        number /= 10;
    }
    printf("Sum of digits of %d is %d\n", originalNumber, sum);

    return 0;
}

    
